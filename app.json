{
  "expo": {
    "name": "Student Enquiry App",
    "slug": "student-enquiry-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "student-enquiry-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.studentenquiryapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.studentenquiryapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#E8D5FF",
          "image": "./assets/images/splash.png",
          "imageWidth": 360,
          "resizeMode": "contain",
          "android": {
            "imageWidth": 280
          },
          "dark": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#E8D5FF",
            "imageWidth": 360
          }
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}
